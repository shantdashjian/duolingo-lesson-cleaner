{
  "manifest_version": 3,
  "name": "Duolingo Lesson Cleaner",
  "description": "A Chrome extension to clean Duolingo lessons of distracting images",
  "version": "0.0.0.1",
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": [
        "https://www.duolingo.com/lesson"
      ]
    }
  ],
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "192": "android-chrome-192x192.png",
    "512": "android-chrome-512x512.png"
  }
}

